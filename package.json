  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "npm-check-updates": "^16.0.5",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/github": "^5.0.3",
    "@types/node": "^18.6.4",
    "axios": "^0.27.2"
  }
}
